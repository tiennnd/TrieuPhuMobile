<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.tientt.trieuphumobile">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/atp_icon_retry"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/atp_icon_retry"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".version2.activity.FirstActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".version2.activity.MainActivity"></activity>
        <activity android:name=".version2.activity.AdsActivity"></activity>
    </application>





</manifest>